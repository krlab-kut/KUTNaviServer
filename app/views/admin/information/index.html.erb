<h1>Admins::Information#index</h1>
<p>Find me in app/views/admins/information/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @information_title.each do |information| %>
      <tr>
        <td><%= information.title %></td>
        <td><%= link_to 'Show', {:controller => "information", :action => "new"}, {:method => :get} %></td>
        <td><%= link_to 'Edit', {:controller => "information", :action => "new"}, {:method => :get} %></td>
        <td><%= link_to 'Destroy', {:controller => "information", :action => "new"}, {:method => :get} %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<p><%= button_to '最新情報新規作成', {:controller => "information", :action => "new"}, {:method => :get} %></p>
