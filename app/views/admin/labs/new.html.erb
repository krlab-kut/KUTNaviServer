<h1>New Lab</h1>

<%= form_for :lab, url: admin_labs_path do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :place_id %>
    <%= f.collection_select :place_id, Place.all, :id, :name, :include_blank => true %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', admin_labs_path %>
